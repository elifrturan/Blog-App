@model List<EntityLayer.Concrete.Post>

<div class="card mb-3">
    <div class="card-header">
        <div class="card-title mb-0">New Posts</div>
    </div>
    <div class="card-body">
        @foreach(var post in Model)
        {
            <div class="mb-3">
                <h6 class="mb-0">
                    <a>@post.Title</a>
                </h6>
            </div>
        }
    </div>
</div>